{
  "manifest_version": 3,
  "name": "VK-AK пользовательский автомат Вконтакте",
  "version": "1.1",
  "description": "Автокликер для добавления друзей ВК со страницы рекомендаций. Разработано @2024 L.A.P.S. Lab",
  "icons": {
    "512": "./assets/VK_AK_512.png",
    "256": "./assets/VK_AK_256.png",
    "128": "./assets/VK_AK_128.png",
    "36": "./assets/VK_AK_36.png"
  },
  "content_scripts": [
    {
      "matches": ["https://vk.com/friends?act=find*"],
      "js": ["vk-friends-clicker-ext.js"]
    }
  ]
}
